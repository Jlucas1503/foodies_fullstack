
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  GUEST
  ADMIN
}

model Category {
  id String @id @default(uuid())
  name String 
  createdAt DateTime @default(now()) @map("created_at")
  @@map("categories")
}